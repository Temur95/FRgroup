<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ulugbek.frgroup">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_fr"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_fr"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Open">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="com.ulugbek.frgroup.LOGIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="com.ulugbek.frgroup.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ProductActivity">
            <intent-filter>
                <category android:name="android.intent.action.ProductActivity" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityClientInfo" />
        <activity android:name=".Register">
            <intent-filter>
                <action android:name="com.ulugbek.frgroup.REGISTER"/>


                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>